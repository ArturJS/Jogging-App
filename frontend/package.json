{
    "name": "jogging-app-frontend",
    "author": "ArturJS",
    "license": "MIT",
    "version": "1.0.0",
    "scripts": {
        "start": "npx cross-env NODE_ENV=production node ./src/server/index.js",
        "start:pm2": "npx pm2 start ./src/server/index.js",
        "dev": "npx cross-env NODE_ENV=development node ./src/server/index.js",
        "debug": "npx ndb cross-env NODE_ENV=development node ./src/server/index.js",
        "build": "cd ./src/client && npx next build",
        "flow": "npx flow",
        "lint": "eslint -c .eslintrc '{src,scripts,local_modules}/**/*.{js,jsx,mjs}'",
        "test": "jest --env=jsdom",
        "test:coverage": "jest --env=jsdom --coverage",
        "precommit": "npm run flow && npm run lint-staged",
        "lint-staged": "npx lint-staged"
    },
    "dependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",

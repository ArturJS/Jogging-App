[ignore]
<PROJECT_ROOT>/node_modules/flow-bin/.*
<PROJECT_ROOT>/node_modules/config-chain/test/.*

[include]
./flow-typed

[libs]

[lints]

[options]
module.name_mapper='.*\(.scss\)' -> 'empty/object'
